<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/LinearLayout1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <RelativeLayout
        android:id="@+id/jiazaiyinchang"
        android:layout_width="match_parent"
        android:layout_height="match_parent" >

        <RelativeLayout
            android:id="@+id/yinchang1"
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:background="@drawable/ddxq"
            android:paddingBottom="10dp"
            android:paddingTop="10dp" >

            <TextView
                android:id="@+id/orderA_name1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_marginLeft="10dp"
                android:layout_toRightOf="@+id/orderA_tuoxiang1"
                android:text="qiang" />

            <TextView
                android:id="@+id/orderA_chepaihao1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignLeft="@+id/orderA_name1"
                android:layout_alignParentBottom="true"
                android:text="E1232123 " />

            <TextView
                android:id="@+id/orderA_haoping1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBottom="@+id/orderA_xinxin1"
                android:layout_toRightOf="@+id/orderA_xinxin1"
                android:background="@drawable/fen"
                android:text="4.0"
                android:textColor="@color/baise" />

            <TextView
                android:id="@+id/orderA_xinxin1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/orderA_name1"
                android:layout_alignBottom="@+id/orderA_name1"
                android:layout_toRightOf="@+id/orderA_name1"
                android:background="@drawable/x4" />

            <com.femto.shipper.view.RoundImageView
                android:id="@+id/orderA_tuoxiang1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:layout_marginLeft="20dp"
                android:src="@drawable/siji" />

            <View
                android:id="@+id/textView1"
                android:layout_width="0.5dp"
                android:layout_height="50dp"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="60dp"
                android:background="@color/huise" />

            <ImageView
                android:id="@+id/orderA_like1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignLeft="@+id/orderA_haoping1"
                android:layout_alignTop="@+id/orderA_yunhou1"
                android:src="@drawable/like1" />

            <ImageView
                android:id="@+id/orderA_zhangkai1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="16dp"
                android:src="@drawable/zhankai1" />

            <TextView
                android:id="@+id/orderA_yunhou1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/orderA_chepaihao1"
                android:layout_alignBottom="@+id/orderA_chepaihao1"
                android:layout_marginLeft="16dp"
                android:layout_toRightOf="@+id/orderA_chepaihao1"
                android:text="接单"
                android:textColor="@color/hongse" />
        </RelativeLayout>
        <!-- fsdhjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjfffffffffffffffffffffffffffffff- -->

        <LinearLayout
            android:id="@+id/ll_layout1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/yinchang1"
            android:background="@drawable/ddxq"
            android:gravity="center"
            android:orientation="horizontal" >

            <LinearLayout
                android:id="@+id/orderA_weizi1"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginLeft="20dp"
                android:gravity="center"
                android:orientation="vertical" >

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/xingcheng" />

                <TextView
                    android:id="@+id/textView7"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="查看行程" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/orderA_lianxi1"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginLeft="20dp"
                android:gravity="center"
                android:orientation="vertical" >

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/lianxi" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="联系司机" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/orderA_gongxiangweizi1"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginLeft="20dp"
                android:gravity="center"
                android:orientation="vertical" >

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/gongxiang" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="共享位置" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/orderA_quxiao1"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginLeft="20dp"
                android:gravity="center"
                android:orientation="vertical" >

                <ImageView
                    android:id="@+id/zhuangtaitu1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/quxiao" />

                <TextView
                    android:id="@+id/quxiao_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="取消订单" />
            </LinearLayout>
        </LinearLayout>
    </RelativeLayout>

</LinearLayout>